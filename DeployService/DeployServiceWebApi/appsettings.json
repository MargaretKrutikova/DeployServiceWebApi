{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "..\\logs\\log-{Date}.txt",
          "retainedFileCountLimit": 10
        }
      }
    ]
  },
  "DeploySettingsPath": "..\\resources\\vds-deploy-settings.json",
  "RepoUpdateScriptPath": "..\\tools\\checkout_svn.bat"
}